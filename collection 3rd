public class Exc{  
    
    public static void main(String[] args) {
        int value  = 5000;
        System.out.println((5000/100)*5);
        System.out.println((5000/100)*10);
        System.out.println((5000/100)*20);
        System.out.println((5000/100)*25);
        System.out.println((5000/100)*50);
    }
}
