public class Exc{  
    
    public static void main(String[] args) {
        System.out.println("Customer Details");
        System.out.println("--------------------");
        ArrayList al = new ArrayList<>();
        al.add(101);
        al.add("Jones");
        al.add('M');
        al.add("customer1@gmail.com");
        al.add(1122334455);
        System.out.println(al);
    }
}

