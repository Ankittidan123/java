---------------------------------------------------------------------- 
 double -> float -> long -> int -> char -> short -> byte  

double --->float 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        float b = (float) a;
        System.out.println(b);
    }
}


double --->long

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        long b = (long) a;
        System.out.println(b);
    }
}


double --->int 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        int b = (int) a;
        System.out.println(b);
    }
}


double --->char

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        char b = (char) a;
        System.out.println(b);
    }
}


