file B
work as data type variable 

Ans)
import java.util.Scanner;

public class Mnc{
    String name;
    String fname;
    String mname;
    String graduate;
    String address;
    String contact;
    String grp_name;
    String semester;
    Mnc(String name,String fname,String mname,String graduate,String address,String contact,String grp_name,String semester){
        this.name = name;
        this.fname = fname;
        this.mname = mname;
        this.graduate = graduate;
        this.address = address;
        this.contact = contact;
        this.grp_name = grp_name;
        this.semester = semester;
    }
}

Second file

import java.util.*;

public class Test{
    int a;
    Mnc k;
    Test(int a,Mnc k){
        this.a = a;
        this.k = k;
    }
    void show(){
        System.out.println(a);
        System.out.println(k.name+" "+k.fname+" "+k.mname);
    }
    public static void main(String[] args) {
        Mnc k = new Mnc("Ragha", "Ankit", "Khushi", null, null, null, null, null);
        Test i =new Test(23, k);
        i.show();
    }
}

---------------------------------------------------------
byte ->short ->int ->long ->float ->double 

byte convert short 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        byte a = 89;
        short b = a;
        System.out.println(b);
    }
}


byte convert int 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        byte a = 89;
        int b = a;
        System.out.println(b);
    }
}


byte convert long

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        byte a = 89;
        long b = a;
        System.out.println(b);
    }
}

 
