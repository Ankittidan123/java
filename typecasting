---------------------------------------------------------------------- 
 double -> float -> long -> int -> char -> short -> byte  

double---->long --->float

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        float b = (long) a;
        System.out.println(b);
    }
}
 
double-->int--->float 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        float b = (int) a;
        System.out.println(b);
    }
}

double--->char--->float 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        float b = (char) a;
        System.out.println(b);
    }
}

double--short--->float 

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        double a = 8.97;
        float b = (short) a;
        System.out.println(b);
    }
}

long --->short--->int

Ans)
import java.util.*;

public class Test{
    
    public static void main(String[] args) {
        long a = 8788768;
        int b = (short) a;
        System.out.println(b);
    }
} 
