user input 
-----------------------------------------------------------------
file A 
employe 

id
name
companyname
department
contact
address 
email
password 
salary 

file B 
work as data type variable 

Ans)
import java.util.Scanner;

public class Mnc{
    String id;
    String name;
    String cname;
    String dname;
    double contact;
    String address;
    String email;
    String pass;
    float salary;
    Mnc(String id,String name,String cname,String dname,double contact,String address,String email,String pass,float salary){
        this.id = id;
        this.name = name;
        this.cname = cname;
        this.dname = dname;
        this.contact = contact;
        this.address = address;
        this.email = email;
        this.pass = pass;
        this.salary = salary;
    }
}

second file

import java.util.*;

public class Test{
    int a;
    Mnc k;
    Test(int a,Mnc k){
        this.a = a;
        this.k = k;
    }
    void show(){
        System.out.println(a);
        System.out.println(k.id+" "+k.name+" "+k.cname);
    }
    public static void main(String[] args) {
        Mnc k = new Mnc("123", "Raghav", "Regex", null, 0, null, null, null, 0);
        Test i =new Test(23, k);
        i.show();
    }
}

