import java.util.Scanner;

public class Mnc{
    String title;
    String auther;
    String type;
    String name;
    String page;
    String content;
    String price;
    String data;
    Mnc(String title,String auther,String type,String name,String page,String content,String price,String data){
        this.title = title;
        this.auther = auther;
        this.type = type;
        this.name = name;
        this.page = page;
        this.content = content;
        this.price = price;
        this.data = data;
    }
}


second file

import java.util.*;

public class Test{
    int a;
    Mnc k;
    Test(int a,Mnc k){
        this.a = a;
        this.k = k;
    }
    void show(){
        System.out.println(a);
        System.out.println(k.title+" "+k.type+" "+k.name);
    }
    public static void main(String[] args) {
        Mnc k = new Mnc("CODER", null, "Best", "Shalendra", null, null, null, null);
        Test i =new Test(23, k);
        i.show();
    }
}

