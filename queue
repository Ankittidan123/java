import java.util.*;

public class Exc {
    public static void main(String[] args) {
        TreeSet<Integer> al = new TreeSet<Integer>();
        al.add(56);
        al.add(13);
        al.add(167);

        PriorityQueue<String> al1 = new PriorityQueue(al);
        al1.add("hello");
        al1.add("dfkj");
        System.out.println(al1);

    }
}

// It can only happen when both have same type of data

