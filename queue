import java.util.*;

public class Exc {
    public static void main(String[] args) {
        PriorityQueue<Integer> al = new PriorityQueue<Integer>();
        al.add(56);
        al.add(13);
        al.add(167);

        TreeSet<String> al1 = new TreeSet(al);
        al1.add("hello");
        al1.add("dfkj");
        System.out.println(al1);

    }
}

//it can only happen when both have same type of data

